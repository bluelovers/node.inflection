    "dasherize",
    "titleize",
    "demodulize",
    "tableize",
    "classify",
    "foreign_key",
    "ordinalize",
    "string"
  ],
  "homepage": "https://github.com/dreamerslab/node.inflection#readme",
  "bugs": {
    "url": "https://github.com/dreamerslab/node.inflection/issues"
